@startuml PROGRESSUS
package users{
    entity User{
        id: integer
        username: varchar(50)
        email: varchar(50)
    }
    entity Profile{
        id: integer
        nome_completo: varchar(50)
        turma: varchar(4)
        usuario: varchar(50)
        fk_id_usuario: integer
    }
    Profile "(1,1)" -- "(1,1)" User
}
@enduml